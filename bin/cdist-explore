. cdist-config

cd ${CDIST_LIBDIR}

for func in cdist_explore_*; do
   #func="cdist_explore_$report"
   var="__$func"

   eval $var=\"\$\($func\)\"
   eval echo $var=\\\"\$$var\\\"
done
